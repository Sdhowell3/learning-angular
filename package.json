{
  "name": "learning-angular",
  "version": "1.0.0",
  "main": "index.js",
  "scripts": {
    "build:clean": "rm -rf ./dist && mkdir ./dist",
    "build:assets": "cp -rf src/assets/* ./dist/",
    "build": "npm run build:clean && npm run build:assets",
    "prestart": "npm run build",
    "start": "node ./node_modules/webpack/bin/webpack.js",
    "preproduction": "rm -rf ./dist && mkdir dist && cp -rf src/assets/* ./dist",
    "production": "NODE_ENV=production node ./node_modules/webpack/bin/webpack.js --config ./production.babel.js",
    "staging": "NODE_ENV=staging node ./node_modules/webpack/bin/webpack.js --config ./production.babel.js",
    "test": "echo \"Error: no test specified\" && exit 1"
  },
  "keywords": [],
  "author": "Scott Howell III",
  "license": "ISC",
  "description": "Learning angular",
  "repository": {
    "type": "git",
    "url": "git+ssh://git@github.com/Sdhowell3/learning-angular.git"
  },
  "bugs": {
    "url": "https://github.com/Sdhowell3/learning-angular/issues"
  },
  "homepage": "https://github.com/Sdhowell3/learning-angular#readme",
  "dependencies": {
    "@angular/common": "^4.4.1",
    "@angular/compiler": "^4.4.1",
    "@angular/core": "^4.4.1",
    "@angular/forms": "^4.4.1",
    "@angular/http": "^4.4.1",
    "@angular/platform-browser": "^4.4.1",
    "@angular/platform-browser-dynamic": "^4.4.1",
    "@angular/router": "4.4.1",
    "bootstrap": "^3.3.7",
    "rxjs": "^5.4.3",
    "zone.js": "^0.8.17"
  },
  "devDependencies": {
    "autoprefixer": "^7.1.4",
    "babel-cli": "^7.0.0-alpha.19",
    "babel-eslint": "^7.0.0-alpha.19",
    "babel-loader": "^7.0.0-alpha.19",
    "babel-plugin-transform-decorators-legacy": "^1.3.4",
    "babel-preset-env": "2.0.0-beta.0",
    "babel-preset-stage-0": "^6.24.1",
    "babel-preset-typescript": "^7.0.0-alpha.19",
    "babel-register": "^7.0.0-alpha.19",
    "babel-runtime": "^7.0.0-alpha.19",
    "browser-sync": "^2.18.13",
    "browser-sync-webpack-plugin": "^1.2.0",
    "eslint": "^4.7.0",
    "eslint-loader": "^1.9.0",
